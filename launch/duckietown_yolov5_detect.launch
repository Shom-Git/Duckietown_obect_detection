<launch>
	<node name="duckietown_yolov5_detect" pkg="duckietown_yolov5" type="detect.py" output="screen"/>
	
	<arg name="img" default="416" doc="Image width(= Height)"/>
	<arg name="weights" default='runs/train/yolov5s_results/weights/best.pt' doc="File directory of weight file"/>
	<arg name="conf" default="0.4"/>
	<arg name="source" default="../content/test/images" doc="File directory of testing images"/>
	
	<param name="param_img" value="$(arg img)"/>
	<param name="param_weights" value="$(arg weights)"/>
	<param name="param_conf" value="$(arg conf)"/>
	<param name="param_source" value="$(arg source)"/>
</launch>

